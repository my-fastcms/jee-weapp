.footer{
	width: 100%;
	background-color: #fff;
	border-top:1rpx solid #FFFFFF;
	height: 99rpx;
	position: fixed;
	bottom: 0;
}
.goodsInfo{
	margin-bottom: 100rpx
}
.footer image{width: 38rpx; height: 38rpx;}
.footer>view{display: inline-block;text-align: center;height: 42px;line-height: 30rpx;}
.footer .index{border-right: 1rpx solid #bbbbbb;}
.footer .index,.footer .collect{width: 14%;}
.footer .buy-only, .footer .buy-group{width: 50%;color: #fff; height:108rpx; padding:10rpx 0; box-sizing: border-box;}
.buy-only{background-color: #fe939d;}
.buy-group{background-color: #ff003c;}
.price-b { font-size:38rpx; font-weight: bold;}

.drawer_screen{width: 100%;height: 100%;position: fixed;top: 0;left: 0;z-index: 100;background-color: #000;opacity: 0.3;overflow: hidden;}
.modal{width: 100%;position: fixed;bottom: 0;z-index: 111;background-color: #fff;height: 70%;}
.modal-close{font-size: 40rpx;color: #5d5d5d;margin: 10rpx 10rpx 0 0;}
.modal-title image{width: 200rpx;height: 200rpx;position: absolute;top: -62rpx;left:24rpx;border-radius: 10rpx;}
.modal .modal-title{padding-left:248rpx;border-bottom: 1px solid #bbbbbb;min-height: 100rpx;}
.modal .goods-price{font-size: 36rpx;color: #e02e24}
.modal .modal-body{padding:20rpx 24rpx; }
.modal .prop{border-bottom: 1px solid #ccc}
.modal .prop-name{padding: 20rpx 0;font-size: 28rpx;}
.modal .prop>text{display: inline-block;background-color: #f2f2f2;border-radius: 10rpx;font-size: 28rpx;line-height: 56rpx;padding: 0 24rpx;margin: 20rpx 30rpx 20rpx 0;}
.selected{background-color: red !important;color: #fff;}
.number{width: 100%;padding: 30rpx 0;border-bottom: 1px solid #ccc;}
.number>text{line-height: 50rpx;}
.buy-value{width: 94rpx;text-align: center;background-color: #eee;margin:0 4rpx;font-size: 24rpx;border-radius: 10rpx;}
.minus,.plus{padding: 0 20rpx; border-radius: 10rpx;background-color: #ccc}
.plus{margin-right: 24rpx;}
.modal .btn{background-color: red;color: #fff;position: absolute;bottom: 0;width: 100%;text-align: center;line-height: 98rpx;border-radius: 0}

.yhq{display: block;padding:0 8px;border: 1px solid #e21323; color:red;font-size: 13px;border-radius: 2px;}
