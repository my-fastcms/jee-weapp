page{
  background-color:#f2f2f2;
}

.container .title{
    font-size: 28rpx;
    -o-text-overflow: ellipsis;
    text-overflow: ellipsis;		  
    overflow: hidden;
    white-space: nowrap;
    width:100%;
    list-style:disc inside none;
}
.container .price{
    font-size: 22rpx;
    display: flex;
    justify-content: space-between;
    color: #626262;
    padding: 10rpx 0 0 0;
}

.progress{
  margin-top: 10rpx;
  font-size: 26rpx;
  background: #fff;
  padding: 10rpx 0;
}
.progress .top{
  display: flex;
  justify-content: flex-start;
  width: 90%;
  margin: 0 auto;
}
.top .jindu{
  line-height:50rpx;
  font-weight: bold;
  font-size: 26rpx;
}
.progress .top image{
  width: 50rpx;
  height: 50rpx;
  border-radius: 50%;
  margin-right: 20rpx;
}
.bar{
  position: relative;
  padding: 20rpx 0;
  box-sizing: border-box;
  width: 86%;
  margin: 0 auto;
}
.parents{
  position: absolute;
  height: 16rpx;
  width: 100%;
  background: #fb6707;
  border-radius: 10rpx;
}
.child{
  position: absolute;
  height: 16rpx;
  background: #bababa;
  border-radius: 10rpx;
  left: 0;
}
.priceBar{
  display: flex;
  justify-content: space-between;
  width: 90%;
  margin: 10rpx auto;
  color: #626262;
}


.stock{
  font-size: 24rpx;
  color: #626262;
  padding: 10rpx;
}
.stock .stockColor{
    color: #f5b141;
}
.tab{
  width:100%;
  background: #fff;
  font-size: 28rpx;
  display: flex;
  justify-content: space-around;
  text-align: center;
  margin-top: 10rpx;
  position: relative;
}
.tabItem{
  width:100%;
  cursor: pointer;
  padding: 20rpx 0;
}
.tabItem.active{
  color: #b52e32;
}


.time{
  padding: 20rpx;
  position: relative;
}
.time text{
  color:#f5b141;
}
.time:after{
    content:" ";
    display: inline-block;
    width:100%;
    position:absolute;
    top:0;
    right:0;
    border:1px solid #e5e5e5;
    -webkit-transform:scaleY(0.5);
    transform:scaleY(0.5);
    -webkit-transform-origin:0 0;
    transform-origin:0 0;
    box-sizing:border-box;
}


